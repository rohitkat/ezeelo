@model IEnumerable<ModelLayer.Models.ViewModel.Save_SiteKeywordSearchViewModel>



<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

<link href="//cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="//cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

<table class="table" id="searchKeyword">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Keyword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SearchResult)
            </th>
            <td>
                @Html.DisplayNameFor(model => model.CityName)
            </td>
            <td>
                @Html.DisplayNameFor(model => model.FranchiseName)
            </td>
            <td>
                @Html.DisplayNameFor(model => model.CategoryName)
            </td>
            <th>
                @Html.DisplayNameFor(model => model.Create_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Network_IP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Device_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeviceType)
            </th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mobile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Keyword)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SearchResult)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CityName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FranchiseName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CategoryName)
                </td>
                
                <td>
                    @Html.DisplayFor(modelItem => item.Create_Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Network_IP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Device_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DeviceType)
                </td>


            </tr>

        }
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $('#searchKeyword').DataTable();

    });
</script>

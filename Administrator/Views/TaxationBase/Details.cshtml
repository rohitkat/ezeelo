                </div>
                <div class="box-content">
                    <div class="form-horizontal">
                        @Html.ValidationSummary(true)
                        @Html.HiddenFor(model => model.ID)

                        <div class="control-group">
                            @*@Html.LabelFor(model => model.TaxationID, "TaxationID", new { @class = "control-label col-md-2" })*@
                            <label class="control-label col-md-2">Tax Name</label>
                            <div class="col-md-10 controls">
                                @Html.Label("TaxationID", (string)ViewBag.TaxationID)
                                @*@Html.ValidationMessageFor(model => model.TaxationID)*@
                            </div>
                        </div>

                        <div class="control-group">
                            @*@Html.LabelFor(model => model.FranchiseTaxDetailID, "FranchiseTaxDetailID", new { @class = "control-label col-md-2" })*@
                            <label class="control-label col-md-2"> Franchise Name</label>
                            <div class="col-md-10 controls">
                                @Html.Label("Franchise", (string)ViewBag.Franchise)
                                @*@Html.ValidationMessageFor(model => model.FranchiseTaxDetailID)*@
                            </div>
                        </div>

                        <div class="control-group">
                            @*@Html.LabelFor(model => model.FranchiseTaxDetailID, "FranchiseTaxDetailID", new { @class = "control-label col-md-2" })*@
                            <label class="control-label col-md-2">Tax Name (Depend ON)</label>
                            <div class="col-md-10 controls">
                                @Html.Label("TaxationDependOn", (string)ViewBag.TaxationDependOn)
                                @*@Html.ValidationMessageFor(model => model.FranchiseTaxDetailID)*@
                            </div>
                        </div>

                        <div class="control-group">
                            @Html.LabelFor(model => model.Description, new { @class = "control-label col-md-2" })
                            <div class="col-md-10 controls">
                                @Html.DisplayFor(model => model.Description)
                                @*@Html.ValidationMessageFor(model => model.Description)*@
                            </div>
                        </div>

                        <div class="control-group">
                            @Html.LabelFor(model => model.IsActive, new { @class = "control-label col-md-2" })
                            <div class="col-md-10 controls">
                                @Html.DisplayFor(model => model.IsActive)
                                @*@Html.ValidationMessageFor(model => model.IsActive)*@
                            </div>
                        </div>


                        <div class="form-actions">
                            <div class="col-md-offset-2 col-md-10">
                              
                                @Html.ActionLink("Back to List", "Index", string.Empty, new { @class = "btn " })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



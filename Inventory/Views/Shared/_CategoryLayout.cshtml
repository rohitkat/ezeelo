    <script>
        $(document).ready(function () {

            //Message box For Success
            var content_Success = '@Session["Success"]';
            if (content_Success == null || content_Success != '') {

                Lobibox.alert("success",
                        {
                            size: 'large',
                            title: '<i class="icon-check" title="Generate Quotation"></i>',
                            delayIndicator: false,
                            msg: content_Success,
                            showClass: 'rollIn',//'zoomInUp',                // Show animation class
                            hideClass: 'rollOut',//'zoomOutDown',               // Hide animation class
                        });
                $.ajax({
                    url: "/Common/Make_SuccessSessionNull",
                    type: "POST",
                    dataType: "json",
                    traditional: true,
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {

                    },
                    error: function () {

                    }
                });
            }
            //Message box For Warning
            var content_Warning = '@Session["Warning"]';
            if (content_Warning == null || content_Warning != '') {

                Lobibox.alert("warning",
                        {
                            size: 'large',
                            title: '<i class="icon-warning-sign" title="Generate Quotation"></i>',
                            delayIndicator: false,
                            msg: content_Warning,
                            showClass: 'rollIn',//'zoomInUp',                // Show animation class
                            hideClass: 'rollOut',//'zoomOutDown',               // Hide animation class
                        });
                $.ajax({
                    url: "/Common/Make_WarningSessionNull",
                    type: "POST",
                    dataType: "json",
                    traditional: true,
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {

                    },
                    error: function () {

                    }
                });
            }
            //Message box For Information
            var content_Info = '@Session["Info"]';
            if (content_Info == null || content_Info != '') {

                Lobibox.alert("info",
                        {
                            size: 'large',
                            title: '<i class="icon-info-sign" title="Generate Quotation"></i>',
                            delayIndicator: false,
                            msg: content_Info,
                            showClass: 'rollIn',//'zoomInUp',                // Show animation class
                            hideClass: 'rollOut',//'zoomOutDown',               // Hide animation class
                        });
                $.ajax({
                    url: "/Common/Make_InfoSessionNull",
                    type: "POST",
                    dataType: "json",
                    traditional: true,
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {

                    },
                    error: function () {

                    }
                });
            }
            //Message box For Error
            var content_Error = '@Session["Error"]';
            if (content_Error == null || content_Error != '') {

                Lobibox.alert("error",
                        {
                            size: 'large',
                            title: '<i class="icon-fire" title="Generate Quotation"></i>',
                            delayIndicator: false,
                            msg: content_Error,
                            showClass: 'rollIn',//'zoomInUp',                // Show animation class
                            hideClass: 'rollOut',//'zoomOutDown',               // Hide animation class
                        });
                $.ajax({
                    url: "/Common/Make_ErrorSessionNull",
                    type: "POST",
                    dataType: "json",
                    traditional: true,
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {

                    },
                    error: function () {

                    }
                });
            }
            //progress bar
            $("#Form_").on("submit", function () {
                Lobibox.progress({
                    title: 'Please wait',
                    label: 'Saving Data..',
                    onShow: function ($this) {
                        var i = 0;
                        var inter = setInterval(function () {
                            window.console.log(i);
                            if (i > 100) {
                                inter = clearInterval(inter);
                            }
                            i = i + 0.8;
                            $this.setProgress(i);
                        }, 10);
                    }
                });
            });
            $("#Email_").on("click", function () {
                Lobibox.progress({
                    title: 'Please wait',
                    label: 'Sending Email..',
                    onShow: function ($this) {
                        var i = 0;
                        var inter = setInterval(function () {
                            window.console.log(i);
                            if (i > 100) {
                                inter = clearInterval(inter);
                            }
                            i = i + 0.08;
                            $this.setProgress(i);
                        }, 10);
                    }
                });
            });
        });
    </script>
    @*End Yashaswi 30/3/2018*@


@model IEnumerable<ModelLayer.Models.EzeeMoneyPayoutDetails>

<script src="~/Content/js/jquery-2.1.3.min.js"></script>
<script src="~/Content/js/jquery.dataTables.min.js"></script>

<script type="text/javascript" class="init">
    $(document).ready(function () {
        $('#tblData').DataTable({
            dom: 'T<"clear">lfrtip',
            "processing": true,
            "serverSide": true,
        });
        $("#expot").click(function () {
            window.location = "/admin/EzeeMoneyPayout/ExportToExcel";
        })
        $("#Back").click(function () {
            window.location = "/admin/EzeeMoneyPayout/Index";
        })
    });
</script>
<br />
<br />
<h1>EzeeMoney Payout Report</h1>
<input id="expot" type="button" value="Export" class="btn btn-success" />
<input id="Back" type="button" value="Back" class="btn btn-blue" />
<table id="tblData" class="table table-striped table-bordered bootstrap-datatable datatable dataTable" >
    <thead>
        <tr>
            <th>Name</th>
            <th>EmailId</th>
            <th>
                PhoneNo
            </th>
            <th>
                QRP
            </th>

            <th>
                Status
            </th>
            <th>
                Delivered Ord Count
            </th>
            <th>
                Total Ord Amount
            </th>
            <th>
                Total Retail Points
            </th>
            <th>
                Total ERP
            </th>
            <th>
                Ezee Money
            </th>
            @*Yashaswi 22-1-19*@
            <th>
                Order Code
            </th>
            @*Yashaswi 22-1-19*@
            <th>
                Transcation Id
            </th>
            <th>Last First Month RP </th>
            <th>Last Secound Month RP </th>
            <th> Total RP </th>

        </tr>
    </thead>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.EmailId
        </td>
        <td>
            @item.PhoneNo
        </td>
        <td>
            @item.QRP
        </td>
        <td>
            @item.Status
        </td>
        <td>
            @item.DelOrdCount
        </td>
        <td>
            @item.TotalOrdAmt
        </td>
        <td>
            @item.TotalRetailPoints
        </td>
        <td>
            @item.ERP
        </td>
        <td>
            @item.EzeeMoney
        </td>
        @*Yashaswi 22-1-19*@
        <td>
            @item.OrdCode
        </td>
        @*Yashaswi 22-1-19*@
        <td>
            @item.TransID
        </td>
        <td> @item.LastFirstMonthRP </td>
        <td> @item.LastSecoundMonthRP </td>
        <td> @(item.LastFirstMonthRP + item.LastSecoundMonthRP)</td>
    </tr>
    }

</table>



@model IEnumerable<ModelLayer.Models.ViewModel.LeadersPayoutRequestViewModel>

<script src="~/Scripts/jquery-1.12.4.js"></script>
<script src="~/Content/js/jquery-2.1.3.min.js"></script>
<script src="~/Content/js/jquery.dataTables.min.js"></script>

<div id="wrap">
    <div class="container">
        <div id="content" class="span10" style="min-height: 403px;">
            @if (Model.Count() == 0)
            {
                <tr>
                    <td colspan="12" style="min-width:960px;max-width:960px">
                        <label style=" color: #138afc; ">No Record Found.... </label>
                    </td>
                </tr>
            }
            else
            {
                <table id="tblData" class="table table-striped table-bordered dt-responsive nowrap" width="70%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.UserLoginID)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Name)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Email)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Mobile)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.EzeeloWalletCash)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.RedeamableCash)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.RequestedAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.GST)%
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.GSTAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.TDSAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.TDS)%
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.MinReserved)%
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.MinReservedAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Penalty)%
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.PenaltyAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ProcessingFees)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ProcessingFeesAmount)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.TotalAmount)
                            </th>
                            <th>
                                IsAllowed
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserLoginID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Name)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Email)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Mobile)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.EzeeloWalletCash)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.RedeamableCash)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.RequestedAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.GST)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.GSTAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TDSAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TDS)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinReserved)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.MinReservedAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Penalty)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.PenaltyAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProcessingFees)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ProcessingFeesAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.TotalAmount)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.isAllowed)
                                </td>
                            </tr>
                        }
                    </tbody>

                </table>
            }
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $('#tblData').DataTable();
    });
</script>

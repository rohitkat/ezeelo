

<script src="~/Scripts/jquery-1.12.4.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

@*<script src="~/Content/js/jquery-2.1.3.min.js"></script>*@

<script src="~/Content/js/jquery.dataTables.min.js"></script>
<link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" />

<script src="https://code.jquery.com/jquery-3.3.1.js"></script>
<script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>


<div class="container">
    <br />
   
    <legend>Leaders Order List</legend>
    <div style="width:50%; margin-right:20px;">
        @*<table id="example" class="display" width="90%">*@

        <table border="0" cellspacing="5" cellpadding="5">
            <tbody>
                <tr>
                    <td>Start Date:</td>
                    <td><input type="datetime" id="fini" name="min"></td>
                </tr>
                <tr>
                    <td>End Date:</td>
                    <td><input type="datetime" id="ffin" name="max"></td>
                </tr>
            </tbody>
        </table>

        <table id="orderTable" class="table table-striped table-bordered dt-responsive nowrap" width="70%" cellspacing="0">
            <thead>
                <tr>
                    <th>Full Name</th>
                    <th>Email ID</th>
                    <th>Ref ID</th>
                    <th>Join Date Ref</th>
                    <th>Status Ref</th>
                    <th>Activate Date Ref</th>
                    <th>Refered ID Ref</th>
                    <th>Requist</th>
                    <th>Requist Active</th>

                    <th>Order Code</th>
                    <th>Order Amount</th>
                    <th>Create Date</th>
                    <th>No. of Point Used</th>
                    <th>Value Per Point</th>
                    <th>Coupen Code</th>
                    <th>Coupen Amount</th>
                    <th>PAN</th>
                    <th>Payment Mode</th>
                    <th>Payable Amount</th>
                    <th>Total Business Point</th>
                    <th>MLM Amount Used</th>
                    <th>Primary Mobile</th>
                    <th>Secondry Mobile</th>
                    <th>Shipping Address</th>
                    <th>Area Name</th>
                    <th>Pincode</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

<script>

    $(document).ready(function () {


        $("#orderTable").DataTable({

            "ajax": {
                url: "/LeadersCustomerOrder/LeadersOrderList",
                type: "POST",
                datatype: "json"
            },

            "columns": [
                { "data": "FullName", "name": "FullName", "autoWidth": true },
                { "data": "EmailID", "name": "EmailID", "autoWidth": true },
                { "data": "Ref_Id", "name": "Ref_Id", "autoWidth": true },
                {
                    "data": "Join_date_ref", "name": "Join_date_ref", "autoWidth": true,
                    "render": function (value) {
                        if (value === null) return "";

                        var pattern = /Date\(([^)]+)\)/;
                        var results = pattern.exec(value);
                        var dt = new Date(parseFloat(results[1]));

                        return (dt.getMonth() + 1) + "/" + dt.getDate() + "/" + dt.getFullYear();
                    }
                },
                { "data": "Status_ref", "name": "Status_ref", "autoWidth": true },
                {
                    "data": "Activate_date_ref", "name": "Activate_date_ref", "autoWidth": true,
                    "render": function (value) {
                        if (value === null) return "";

                        var pattern = /Date\(([^)]+)\)/;
                        var results = pattern.exec(value);
                        var dt = new Date(parseFloat(results[1]));

                        return (dt.getMonth() + 1) + "/" + dt.getDate() + "/" + dt.getFullYear();
                    }
                },

                { "data": "Refered_Id_ref", "name": "Refered_Id_ref", "autoWidth": true },
                 { "data": "request", "name": "request", "autoWidth": true },
                  { "data": "request_active", "name": "request_active", "autoWidth": true },

                { "data": "OrderCode", "name": "OrderCode", "autoWidth": true },
                { "data": "OrderAmount", "name": "OrderAmount", "autoWidth": true },
                {
                    "data": "CreateDate", "name": "CreateDate", "autoWidth": true,
                    "render": function (value) {
                        if (value === null) return "";

                        var pattern = /Date\(([^)]+)\)/;
                        var results = pattern.exec(value);
                        var dt = new Date(parseFloat(results[1]));

                        return (dt.getMonth() + 1) + "/" + dt.getDate() + "/" + dt.getFullYear();
                    }
                },

                { "data": "NoOfPointUsed", "name": "NoOfPointUsed", "autoWidth": true },
                { "data": "ValuePerPoint", "name": "ValuePerPoint", "autoWidth": true },
                { "data": "CoupenCode", "name": "CoupenCode", "autoWidth": true },
                { "data": "CoupenAmount", "name": "CoupenAmount", "autoWidth": true },
                { "data": "PAN", "name": "PAN", "autoWidth": true },
                { "data": "PaymentMode", "name": "PaymentMode", "autoWidth": true },
                { "data": "PayableAmount", "name": "PayableAmount", "autoWidth": true },
                { "data": "BusinessPointsTotal", "name": "BusinessPointsTotal", "autoWidth": true },
                { "data": "MLMAmountUsed", "name": "MLMAmountUsed", "autoWidth": true },
                { "data": "PrimaryMobile", "name": "PrimaryMobile", "autoWidth": true },
                { "data": "SecondoryMobile", "name": "SecondoryMobile", "autoWidth": true },
                { "data": "ShippingAddress", "name": "ShippingAddress", "autoWidth": true },
                { "data": "AreaName", "name": "AreaName", "autoWidth": true },
                { "data": "Pincode", "name": "Pincode", "autoWidth": true },

            ]

        });
    });



    $('#btnExcel').click(function () {

        window.location = '/LeadersCustomerOrder/ExportToExcel';


    })


</script>
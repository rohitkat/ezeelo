<script type="text/javascript">
    $(document).ready(function () {
        $("#Pincode").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/LeadersProfile/GetPincode",
                    type: "GET",
                    dataType: "json",
                    data: { Prefix: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {

                            return { label: item.Name, value: item.ID };
                        }))

                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    });


    $('#Pincode').change(function () {
    
        $.ajax({
            type: "post",
            url: "/LeadersProfile/GetCityDistrictState",
            data: { pincode: $('#Pincode').val() },
            datatype: "json",

            success: function (data) {
                
                $('#City').text(data.CityName);
                $("#District").text(data.districtName);
                $("#State").text(data.stateName);


            },
            error: function () {
            alert("Error occured!!")
        }
        });
    });


</script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#DOB").datepicker({

            dateFormat: 'mm/dd/yy',
            changeMonth: true,
            changeYear: true,
            yearRange: '-100:+0',

            
            onSelect: function (dateText) {
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("AgeValidate", "LeadersProfile")",
                    contentType: "application/json; charset=utf-8",
                   data: '{"DOB":"' + $("#DOB").val() + '"}',
                   dataType: "html",

                success: function (data, status, xhr) {
                    if (data == 'false') {
                        alert("Age Should Be Greater Than 18 Years!!")
                        $(':input[type="submit"]').prop('disabled', true);

                    }
                    else {
                        $(':input[type="submit"]').prop('disabled', false);
                    }


                },
                error: function (xhr, status, error) {
                    //$("#dataDiv").html("Result: " + status + " " + error + " " + xhr.status + " " + xhr.statusText)
                }
            });

        }
        });



    });
</script>








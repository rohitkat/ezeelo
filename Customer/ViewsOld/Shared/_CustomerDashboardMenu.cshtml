
<style>
    .product-sm-left .product-caption-title {
        font-size: 11px;
        height: 35px;
        font-weight: 700;
    }

    .product-sm-left .product-img-wrap {
        padding: 10px 0px;
    }

    #Gender {
        margin-left: 30px;
    }
</style>

<div class="row" data-gutter="15">
    @{
        var sessionID = Session["UID"];
        string cityName = "";
        int franchiseID = 0;////added
        if (Request.Cookies["CityCookie"] != null && Request.Cookies["CityCookie"].Value != "")
        {
            cityName = Request.Cookies["CityCookie"].Value.Split('$')[1].ToLower();
            franchiseID = Convert.ToInt32(Request.Cookies["CityCookie"].Value.Split('$')[2]);////added
        }
    }

    @if (Request.QueryString["pUserLoginID"] != null)
    {
        sessionID = Request.QueryString["pUserLoginID"];
    }

    <div class="col-xs-4 col-sm-2 col-md-1 ">
        <a href="@Url.RouteUrl("my-profile", new { id = sessionID, city = cityName, franchiseId = franchiseID })">
            <div class="product product-sm-left animated fadeRight">
                <div class="product-img-wrap" style="width: 100%; cursor: pointer;">
                    <i class="fa fa-user fa-4x" aria-hidden="true"></i>
                    <h5 class="product-caption-title">My Profile</h5>
                    @*<img class="" src="img/test_product/woman_running_shoes/2.jpg" alt="Image Alternative text" title="Image Title">*@
                </div>
            </div>
        </a>
    </div>
    <div class="col-xs-4 col-sm-2 col-md-1 ">
        <a href="@Url.RouteUrl("CustomerOrder/myorders", new { city = cityName, franchiseId = franchiseID })">
            <div class="product product-sm-left animated fadeRight">
                <div class="product-img-wrap" style="width: 100%; cursor: pointer;">
                    <i class="fa fa-list fa-4x" aria-hidden="true"></i>
                    <h5 class="product-caption-title">My Orders</h5>
                    @*<img class="" src="img/test_product/woman_running_shoes/2.jpg" alt="Image Alternative text" title="Image Title">*@
                </div>
            </div>
        </a>
    </div>
    <div class="col-xs-4 col-sm-2 col-md-1">
        <a href="@Url.RouteUrl("Refer", new { city = cityName, franchiseId = franchiseID })">
            <div class="product product-sm-left animated fadeRight">
                <div class="product-img-wrap" style="width: 100%; cursor: pointer;">
                    <i class="fa fa-group fa-4x" aria-hidden="true"></i>

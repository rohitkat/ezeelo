                    @*<img class="" src="img/test_product/woman_running_shoes/2.jpg" alt="Image Alternative text" title="Image Title">*@
                </div>
            </div>
        </a>
    </div>
    <div class="col-xs-4 col-sm-2 col-md-1">
        <a href="@Url.RouteUrl("MyWishList", new { city = cityName, franchiseId = franchiseID })">
            <div class="product product-sm-left animated fadeRight">
                <div class="product-img-wrap" style="width: 100%;cursor: pointer;">
                    <i class="fa fa-list-alt fa-4x" aria-hidden="true"></i>
                    <h5 class="product-caption-title">Wishlist</h5>
                    @*<img class="" src="img/test_product/woman_running_shoes/2.jpg" alt="Image Alternative text" title="Image Title">*@
                </div>
            </div>
        </a>
    </div>
    <div class="col-xs-4 col-sm-2 col-md-1">
        <a href="@Url.RouteUrl("ChangePwd", new { city = cityName, pUserLoginID=Session["UID"], franchiseId = franchiseID })">
            <div class="product product-sm-left animated fadeRight">
                <div class="product-img-wrap" style="width: 100%; cursor: pointer;">
                    <i class="fa fa-unlock fa-4x" aria-hidden="true"></i>
                    <h5 class="product-caption-title">Change Password</h5>
                    @*<img class="" src="img/test_product/woman_running_shoes/2.jpg" alt="Image Alternative text" title="Image Title">*@
                </div>
            </div>
        </a>
    </div>

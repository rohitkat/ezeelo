<script>
    function showSearch() {
        var charm = $("#charmSearch");
        if (charm.data('hidden') == undefined) { charm.data('hidden', true); }

        if (!charm.data('hidden')) {
            charm.animate({
                right: -300
            });
            $("#showSearch").animate({ right: 0 });
            charm.data('hidden', true);
        }
        else {
            charm.animate({
                right: 0
            });
            $("#showSearch").animate({ right: 303 });
            charm.data('hidden', false);
        }
    }
</script>

    <script src="~/Content/js/classie.js" defer="defer"></script>
    <script>
        function init() {
            window.addEventListener('scroll', function (e) {
                var distanceY = window.pageYOffset || document.documentElement.scrollTop,
                    shrinkOn = 300,
                    header = document.querySelector("#smallHeader");
                topHeader = document.querySelector(".top-header");
                categoryMenu = document.querySelector(".box-vertical-megamenus");
                if (distanceY > shrinkOn) {
                    classie.add(header, "smaller");
                    classie.add(topHeader, "smallerTop");
                    classie.add(categoryMenu, "smallerCategory")
                } else {
                    if (classie.has(header, "smaller")) {
                        classie.remove(header, "smaller");
                        classie.remove(topHeader, "smallerTop");
                        classie.remove(categoryMenu, "smallerCategory")
                    }
                }
            });
        }
        window.onload = init();
    </script>


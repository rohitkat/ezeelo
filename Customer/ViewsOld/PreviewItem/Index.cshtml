        @*Js for loading stock varient*@
        <script type="text/javascript">
            function opr(a) {

                $("#ajax_loading_message").css("display", "block");

                var id = a.id;
                var arr = id.split("+");
                var colorID = 0, sizeID = 0, dimensionID = 0, materialID = 0;
                if (arr[0] == "c") {
                    colorID = arr[1];
                    sizeID = 0;
                    dimensionID = 0;
                    materialID = 0;
                }
                else if (arr[0] == "s") {
                    colorID = $("#ColorID").val();
                    if (colorID <= 0)
                        colorID = $("#SelectedColor").val();

                    sizeID = arr[1];
                    dimensionID = 0;
                    materialID = 0;
                }
                else if (arr[0] == "d") {
                    colorID = $("#ColorID").val();
                    if (colorID <= 0)
                        colorID = $("#SelectedColor").val();
                    sizeID = $("#SizeID").val();
                    if (sizeID <= 0)
                        sizeID = $("#SelectedSize").val();
                    dimensionID = arr[1];
                    materialID = 0;
                }
                else if (arr[0] == "m") {
                    colorID = $("#ColorID").val();
                    if (colorID <= 0)
                        colorID = $("#SelectedColor").val();
                    sizeID = $("#SizeID").val();
                    if (sizeID <= 0)
                        sizeID = $("#SelectedSize").val();
                    dimensionID = $("#DimensionID").val();
                    if (dimensionID <= 0)
                        dimensionID = $("#SelectedDimension").val();
                    materialID = arr[1];
                }
                var itemID = $("#PID").val();
                // alert(itemID);
                var shopID = $("#hdnShopId").val();
                var shopStockID = $("#ShopStockID").val();
                // alert(shopStockID);
                //-- Added for Dynamic Message Ashishg --//
                var MinOrder = '@ViewBag.MinOrderBuyNow';/// $("#hdnMinOrderBuyNow").val();
                var WishListStatus = '@ViewBag.WishListStatus'; //Added for adding varient for WishList Ashish
                //alert(MinOrder);
                $.ajax({
                    url: '@Url.Action("RebindStockVarient", "PreviewItem")',
                    data: { 'itemID': itemID, 'shopID': shopID, 'shopStockID': shopStockID, 'selectedType': arr[0], 'selectedID': arr[1], 'ColorID': colorID, 'SizeID': sizeID, 'DimensionID': dimensionID, 'MaterialID': materialID, 'MinOrder': MinOrder },//-- Added MinOrder for Dynamic Message --//
                    type: "GET",
                    cache: false,
                    success: function (result) {
                        //alert(result);
                        $("#ajax_loading_message").css("display", "none");


                        //window.parent.$("#divStockVarient").html(result);
                        $("[id*=divStockVarient]").html(result);
                        // alert($("#divStockVarient").html());
                        if ($("#hdnOfferCount").val() > 0) {
                            $("#spnOfferCount").text($("#hdnOfferCount").val() + " Offer");
                            $("#divOfferCount").css("display", "block");
                            $("#divOfferDiv").css("display", "block");
                        }
                        else {
                            $("#divOfferCount").css("display", "none");
                            $("#divOfferDiv").css("display", "none");
                        }
                        // Set size of payable amount
                        if ($("#spanPayableMain").html() != "" && $("#spanPayableMain").html().length > 0) {
                            $("#divSaleRate").removeClass("priceDiv");
                            $("#spanPayableMain").addClass("priceDiv");
                        }
                    },
                    failure: function (data) {
                        //alert("Fail");
                        $("#ajax_loading_message").css("display", "none");
                    },
                    error: function (data) {
                        $("#ajax_loading_message").css("display", "none");
                    }
                });
                var pincodeCookie = getCookie("DeliverablePincode");
                //alert(pincodeCookie);
                if (pincodeCookie != null && pincodeCookie != "") {
                    $("[id=divPincodeLink]").css("display", "block");
                    $("[id=divPincodeVerification]").css("display", "none");
                }
                else {
                    $("[id=divPincodeLink]").css("display", "none");
                    $("[id=divPincodeVerification]").css("display", "block");
                }
            }
            function getCookie(name) {
                var dc = document.cookie;
                var prefix = name + "=";
                var begin = dc.indexOf("; " + prefix);
                if (begin == -1) {
                    begin = dc.indexOf(prefix);
                    if (begin != 0) return null;
                } else {
                    begin += 2;
                }
                var end = document.cookie.indexOf(";", begin);
                if (end == -1) {
                    end = dc.length;
                }
                return unescape(dc.substring(begin + prefix.length, end));
            }
        </script>

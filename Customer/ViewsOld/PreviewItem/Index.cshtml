            function GetSimilarProducts() {

                if (productPageIndex == 0) {
                    productPageIndex++;
                }
                // $("#loader").show();

                var mydata = {
                    "cityID": $("#CityID").val(),
                    "franchiseID": $("#FranchiseID").val(),////added
                    "categoryID": $("#CID").val(),

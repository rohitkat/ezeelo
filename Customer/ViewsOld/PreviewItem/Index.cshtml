                }
                else if (productPageIndex == productPageCount) {
                    $("#btnNext").attr("disabled", "disabled");
                    $("#btnPrevious").removeAttr("disabled");
                }
                else if (productPageIndex == 1 && productPageIndex < productPageCount) {
                    $("#btnNext").removeAttr("disabled");
                    $("#btnPrevious").attr("disabled", "disabled");
                }
                else if (productPageIndex < productPageCount) {
                    $("#btnNext").removeAttr("disabled");
                    $("#btnPrevious").removeAttr("disabled");
                }
                else {
                    $("#btnNext").removeAttr("disabled");
                    $("#btnPrevious").removeAttr("disabled");
                }
                $("#tableSimilarProduct").html("");

                $.each(response, function (j, dataVal) {
                    var pid = dataVal.ProductID;
                    var prodName = "";
                    var imgPath = "";
                    var mrpSalerateString = "";
                    if (dataVal.Name.length > 55) {
                        // prodName = dataVal.Name.substring(0, 52) + "...";
                        //***** SEO URL Structure RULE by Ashish 18/11/2016 ******//
                        prodName = dataVal.Name.substring(0, 52).replace("+", " ") + "...";

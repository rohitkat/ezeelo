
@model ModelLayer.Models.Merchant
<div id="modalShowMerchantRatings" class="modal" style="background: rgba(130, 129, 129, 0.77); ">
    <div class="modal-dialog modal-lg">
        <!-- Modal content -->
        <div class="modal-content" style="padding:25px;">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span class="close"><i class="fa fa-close"></i></span></button>
            <div id="divRatingsList" class="row">
                @Html.Partial("_ShowAllRatingsList", new List<ModelLayer.Models.MerchantRating>())
            </div>
        </div>
    </div>
</div>

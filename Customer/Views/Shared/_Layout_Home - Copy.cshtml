    <script>
        $(document).ready(function () {
            $(window).scroll(function () {
                if ($(this).scrollTop() > 50) {
                    $('#back-to-top').fadeIn();
                } else {
                    $('#back-to-top').fadeOut();
                }
            });
            // scroll body to 0px on click
            $('#back-to-top').click(function () {
                $('#back-to-top').tooltip('hide');
                $('body,html').animate({
                    scrollTop: 0
                }, 800);
                return false;
            });
            $('#back-to-top').tooltip('show');
            // Onclick menubutton code
            $('.navbar-toggle').click(function () {
                $('body,html').animate({
                    scrollTop: 0
                }, 800);
            });

            //Start 18/12/2018 pincode refresh button
            $("#RefreshCityCookies").click(function () {
                var oldAreaID = localStorage.getItem("AreaId");
                localStorage.removeItem("CityID");
                localStorage.removeItem("PinArea");
                localStorage.removeItem("AreaId");
                localStorage.removeItem("SelectedPincode");
                localStorage.removeItem("IsLocConfirmed");

                var cookies = document.cookie.split(";");
                for (var i = 0; i < cookies.length; i++) {
                    var equals = cookies[i].indexOf("=");
                    var name = equals > -1 ? cookies[i].substr(0, equals) : cookies[i];
                    document.cookie = name + "=;expires=Thu, 01 Jan 1970 00:00:00 GMT";
                }

                var changeUrl = '@(Html.Raw(Url.RouteUrl("ChangeCity", new { city = cityName, CityID = "_CityID_", CityName = "_CityName_", FranchiseID = "_FranchiseID_", HelpLineNumber = "_HelpLineNumber_", OldAreaId = "OldAreaId" })))';
                changeUrl = changeUrl.replace("_CityID_", "@System.Configuration.ConfigurationManager.AppSettings["DFLT_CITY_ID"]").replace("_CityName_", "@System.Configuration.ConfigurationManager.AppSettings["DFLT_CITY_NAME"]").replace("_FranchiseID_", "@System.Configuration.ConfigurationManager.AppSettings["DFLT_FRANCHISE_ID"]").replace("_HelpLineNumber_", "@System.Configuration.ConfigurationManager.AppSettings["DFLT_HELPLINE_NO"]").replace("OldAreaId", oldAreaID);                                                       

                window.location.href = changeUrl;
            });
            //End 18/12/2018 pincode refresh button
        });
    </script>

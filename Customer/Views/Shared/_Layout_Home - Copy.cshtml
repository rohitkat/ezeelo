                        var LocStorSelectedPincode;
                        //End
                        var modalPinAreaChange = document.getElementById('divPinAreaChange');
                        var modalPinArea = document.getElementById('divPinArea');
                        var modaldivCheckLoc = document.getElementById('divCheckLoc');
                        //End
                        // Get the modal
                        var modal = document.getElementById('myModal');

                        // Get the button that opens the modal
                        var btn = document.getElementById("myBtn");

                        // Get the <span> element that closes the modal
                        var span = document.getElementsByClassName("close")[0];

                        // When the user clicks the button, open the modal
                        btn.onclick = function () {
                            modal.style.display = "block";
                            //********* Multiple Franchise in same city By Ashish***********//
                            if ($("#ddlSelectCity").val() !== null) {
                                $("#lblarea").text('');
                            }
                            $("#ddlSelectCity").val(0);
                            //Start Yashaswi 30-11-2018 Pincode Selection

                            //$("#txtSelectPincode").hide();
                            $("#ddlSelectArea").hide();
                            //$("#txtSelectPincode").val('');
                            $("#SignUp").hide();
                            $("#ddlSelectArea").val('');
                            $("#divSearchPicode").hide();
                            $("#txtPincodeSearch").val('');

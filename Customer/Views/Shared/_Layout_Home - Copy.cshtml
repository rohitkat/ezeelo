                            if (localStorage.getItem("CityID") != null) {
                                $("#ddlSelectCity").val(localStorage.getItem("CityID"));
                                $("#ddlSelectCity").change();
                            }
                            //
                        }

                        // When the user clicks on <span> (x), close the modal
                        span.onclick = function () {
                            modal.style.display = "none";
                        }

                        // When the user clicks anywhere outside of the modal, close it
                        window.onclick = function (event) {
                            //if (event.target == modal) {
                            //    modal.style.display = "none";
                            //}
                            //Yashaswi
                            if (event.target == modalPinAreaChange) {
                                modalPinAreaChange.style.display = "none";
                            }
                            if (event.target == modalPinArea) {
                                modalPinArea.style.display = "none";
                            }
                            if (event.target == modaldivCheckLoc) {
                                modaldivCheckLoc.style.display = "none";
                            }
                            //End
                        }
                        ////added by ashish
                        var startshopBtn = document.getElementById('startshoppingID');
                        startshopBtn.onclick = (function () {
                            modal.style.display = "none";
                        });
                        //Yashaswi
                        function CancelChangeAreaConfirmWindow() {
                            modalPinAreaChange.style.display = "none";

                        }
                        function ContinueChangeAreaConfirmWindow() {
                            localStorage.removeItem("IsLocConfirmed");
                            localStorage.setItem("CityID", LocStorddlSelectCityI);
                            localStorage.setItem("PinArea", LocStorPinArea);
                            localStorage.setItem("AreaId", LocStorAreaId);
                            localStorage.setItem("SelectedPincode", LocStorSelectedPincode);
                            window.location.href = UrlAfterConfirm;
                        }
                        //End



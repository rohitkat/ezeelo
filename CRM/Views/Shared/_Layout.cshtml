    <script type="text/javascript">
        $('input[type=text]').each(function () {
            var req = $(this).attr('data-val-required');
            if (undefined != req) {
                //alert($(this).closest(".controls").parent().find('.control-label').text());
                //var label = $('label[for="' + $(this).attr('id') + '"]');
                var label = $(this).closest(".controls").parent().find('.control-label');
                //var label = req.closest().find('.control-label');// $('.control-label');
                var text = label.text();
                //alert(text);
                if (text.length > 0) {
                    label.append('<span style="color:red"> *</span>');
                }
            }
        });

        $("#btn_clear").click(function () {
            //$('#FromDate').val('');
            //$('#ToDate').val('');
            //$('.search').val('');
            $('form input').val("");
            $('form select').val("");
        })


        $(document).ready(function () {
            $.ajax({
                url: '@Url.Action("CountPending", "CustomerOrder")',
                type: "GET",
                success: function (result) {
                    $('#notification').html("Transaction Pending : " + result);
                    //alert();

                },
                error: function (data) {
                    //alert('Failure');
                }
            });
        });

    </script>


    <!-- end: Favicon -->
    <style>
        body {
            background: url(/Content/img/mer-bg.jpg) repeat 0px 0px;
        }
        .planExpry {
            color: #ff000a;
            font-size: 19px;
            text-shadow: 1px 1px 2px rgb(255, 9, 9);
            text-align:center;
        }
    </style>

</head>
<body>
    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand logo span2" href="@Url.Action("Index","Home")">
                    <img src="~/Content/Logo/logo1.png" />
                </a>
                <div class="span6">
                    <h1> @Html.Label(@Session["ShopName"].ToString(), new { @class = "pull-right shopName" })</h1>
                  
                  
                </div>
               

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav sapn4">
                    <ul><li><iframe src='@Url.Action("ShopLogoDisplay", "ProductUploadTemp")' frameborder="0" width="60px" height="70px" id="iframePersonalDetail" class="tab-pane active"></iframe></li></ul>
                    <ul class="nav pull-right">
                        <li class="dropdown set-list">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                @Session["USER_NAME"].ToString()
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="dropdown-menu-title">
                                    <span>Account Settings</span>
                                </li>
                                <li><a href="@Url.Action("Edit", "PersonalDetail")"><i class="halflings-icon user"></i>Profile</a></li>
                                <li><a href="@Url.Action("MerchantProfileUpload", "ShopProfile")"><i class="halflings-icon user"></i>Profile Image</a></li>
                                <li><a href="@Url.Action("LogOff", "MerchantRegister")"><i class="halflings-icon off"></i>Logout</a></li>

                            </ul>
                        </li>
                        <!-- end: User Dropdown -->
                    </ul>
                </div>
                <!-- end: Header Menu -->

            </div>
            <div class="planExpry">
                <marquee direction="right" behavior="alternate">
                    @if (Session["PlanExpDaysRem"] != null)
                    {
                        <p>Your plan will be expire after @Session["PlanExpDaysRem"].ToString() days. Please renew your plan</p>
                    }
                </marquee>
            </div>

         
        </div>
    </div>
    <!-- start: Container-->
    <div class="container-fluid-full">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div id="sidebar-left" class="span2">
                <div class="nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li><a href=@Url.Action("Index", "Home")><i class="icon-bar-chart"></i><span class="hidden-tablet">Dashboard</span></a></li>
                        <li>
                            <a href=@Url.Action("", "") class="dropmenu"><i class="icon-shopping-cart" title="Track Order"></i><span class="hidden-tablet">Track Order</span></a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("Index","Placed")><i class="icon-exclamation-sign" title="Pending"></i><span class="hidden-tablet">Pending</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Confirm")><i class="icon-ok" title="Confirm"></i><span class="hidden-tablet">Confirm </span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Packed")><i class="icon-gift" title="Packed"></i><span class="hidden-tablet">Packed </span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Dispatched")><i class="icon-home" title="Dispatched From Shop"></i><span class="hidden-tablet">Dispatched From Shop</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Delivered")><i class="icon-truck" title="Delivered"></i><span class="hidden-tablet">Delivered</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Returned")><i class="icon-share-alt" title="Returned"></i><span class="hidden-tablet">Returned </span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","Cancelled")><i class="icon-remove" title="Cancelled"></i><span class="hidden-tablet">Cancelled </span></a></li>
                            </ul>


                        </li>
                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-tags" title="Product"></i><span class="hidden-tablet"> Product</span></a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("Index","ProductUploadTemp")><i class="icon-file-alt" title="Product Upload"></i><span class="hidden-tablet">Product Upload</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index", "ProductUploadAproved")><i class="icon-file-alt" title="Approved Product List"></i><span class="hidden-tablet">Approved Product List</span></a></li>
                            </ul>
                        </li>
                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-money" title="Shop Component Price"></i><span class="hidden-tablet">Shop Component Price</span></a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("Index","ShopComponentPrice")><i class="icon-money" title="Shop Component Price"></i><span class="hidden-tablet">Shop Component Price</span></a></li>
                            </ul>
                        </li>

                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-file-alt" title="MIS Report"></i><span class="hidden-tablet">MIS Report</span>@*<span class="label label-important">9</span>*@</a>
                            <ul>

                                <li><a class="submenu" href=@Url.Action("Index","OrderTransactionReport")><i class="icon-paste" title="Order Transaction"></i><span class="hidden-tablet">Order Transaction</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index","ProductApprovedNonApproved")><i class="icon-ok-sign" title="Product Approval"></i><span class="hidden-tablet">Product Approval</span></a></li>
                                @*<li><a class="submenu" href=@Url.Action("Index","ShopSaleReport")><i class="icon-user-md" title="Shop/Merchant Sale"></i><span class="hidden-tablet">Shop/Merchant Sale</span></a></li>*@
                                <li><a class="submenu" href=@Url.Action("Index","StockManagementReport")><i class="icon-user-md" title="Stock Management"></i><span class="hidden-tablet">Stock Management</span></a></li>

                            </ul>
                        </li>
                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-user" title="Empolyee Management"></i><span class="hidden-tablet">Empolyee Management</span>@*<span class="label label-important">9</span>*@</a>
                            <ul>

                                <li><a class="submenu" href=@Url.Action("Index","EmployeeManagement")><i class="icon-user" title="Empolyee Management"></i><span class="hidden-tablet">Empolyee Management</span></a></li>

                            </ul>
                        </li>
                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-home" title="Shop Management"></i><span class="hidden-tablet">Shop Management</span>@*<span class="label label-important">9</span>*@</a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("ShopImageUpload", "ShopProfile")><i class="icon-upload" title="Upload Shop Images"></i><span class="hidden-tablet">Upload Shop Images</span></a></li>
                                <li><a class="submenu" href=@Url.Action("ShopLogoUpload", "ProductUploadTemp")><i class="icon-upload" title="Upload shop Logo"></i><span class="hidden-tablet">Upload shop Logo</span></a></li>
                            </ul>
                        </li>

                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-leaf" title="Offer Management"></i><span class="hidden-tablet">Offer Management</span></a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("Index", "MasterOffer")><i class="icon-check" title="Master Offer"></i><span class="hidden-tablet">Master Offer</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index", "OfferDuration")><i class="icon-check" title="Offer Duration"></i><span class="hidden-tablet">Offer Duration</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index", "OfferZone")><i class="icon-check" title="Offer Zone"></i><span class="hidden-tablet">Offer Zone</span></a></li>



                            </ul>
                        </li>
                        <li>
                            <a class="dropmenu" href=@Url.Action("","")><i class="icon-tags" title="Component Offer Management"></i><span class="hidden-tablet">Component Offer Management</span></a>
                            <ul>
                                <li><a class="submenu" href=@Url.Action("Index", "MasterComponentOffer")><i class="icon-edit" title="Master Component Offer"></i><span class="hidden-tablet">Master Component Offer</span></a></li>
                                <li><a class="submenu" href=@Url.Action("Index", "ComponentOfferDuration")><i class="icon-edit" title="Component Offer Duration"></i><span class="hidden-tablet">Component Offer Duration</span></a></li>

                                <li><a class="submenu" href=@Url.Action("Index", "ComponentOfferZone")><i class="icon-edit" title="Component Offer Zone"></i><span class="hidden-tablet">Component Offer Zone</span></a></li>



                            </ul>
                        </li>


                    </ul>
                </div>
            </div>
            <!-- end: Main Menu -->

            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="#" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
            </noscript>


            <div class="container body-content">
                @RenderBody()
            </div>
        </div><!--/#content.span10-->
    </div><!--/fluid-row-->
    <!-- start: Footer-->
    <footer>

        <p>&copy; @DateTime.Now.Year - eZeelo</p>
        <script type="text/javascript">
            var width = $(window).width();
            if (width < 480) {
                var url = window.location.href;
                document.write('<a href="whatsapp://send" data-text="' + url + '" data-href="" class="wa_btn wa_btn_s" style="display"><button class=""><img  src="~/Content/img/whatsapp.png" /></button></a>');
            }
        </script>

    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <!-- start: JavaScript-->
    <!-- start: JavaScript-->

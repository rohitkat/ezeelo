<!DOCTYPE html>
<html>
<head>

    @*<meta http-equiv="Pragma" content="no-cache">
        <meta http-equiv="Expires" content="-1" >
        <meta http-equiv="CACHE-CONTROL" content="NO-CACHE" >*@

    <!-- start: Meta -->
    <meta charset="utf-8">
    <title>Admin.eZeelo</title>
    <meta name="description" content="Bootstrap Metro Dashboard">
    <meta name="author" content="Dennis Ji">
    <meta name="keyword" content="Metro, Metro UI, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <!-- end: Meta -->
    <!-- start: Mobile Specific -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- end: Mobile Specific -->
    <!-- start: CSS -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&subset=latin,cyrillic-ext,latin-ext' rel='stylesheet' type='text/css'>
    <!-- end: CSS -->
    <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <link id="ie-style" href="css/ie.css" rel="stylesheet">
    <![endif]-->
    <!--[if IE 9]>
        <link id="ie9style" href="css/ie9.css" rel="stylesheet">
    <![endif]-->
    <link href="~/Content/css/Site.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/Content/css/style.css" rel="stylesheet" />
    <link href="~/Content/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/css/halflings.css" rel="stylesheet" />
    <link href="~/Content/css/Site.css" rel="stylesheet" />
    @*<link href="~/Content/css/jquery.cleditor.css" rel="stylesheet" />*@
    <!-- start: Favicon -->
    @*<link rel="shortcut icon" href="~/Content/Logo/logo.png">*@
    <!-- end: Favicon -->



    <style type="text/css">
        #pageloaddiv {
            position: fixed;
            left: 0px;
            top: 0px;
            width: 100%;
            height: 100%;
            z-index: 1000;
            background: url('../Content/Images/ajax_loader.gif') no-repeat center center;
            background-size: 100px;
            display: none;
        }

        .logout-link {
            display: inline-block !important;
        }

        .dropdown-menu .halflings-icon.off {
            background-position: -379px 2px;
            padding: 1px 6px;
            display: inline-block;
        }

        .container {
            margin-left: 0 !important;
        }
    </style>

</head>
<body>
    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">

                <div id="pageloaddiv"></div>



                <a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="gandhibaghLogo" href="@Url.Action("Index", "Home")">

                    <img src="~/Content/Images/logo.png" alt="Admin eZeelo" />
                </a>

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav">
                    <ul class="nav pull-right">
                        <li class="dropdown">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="halflings-icon white user">

                                </i>@*Admin*@
                                @if (Session["RoleName"] != null)
                                {
                                    @Html.Label(Session["RoleName"].ToString(), new { @style = "display:inline-block;" });
                                }
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">

                                @*<li><a href="#"><i class="halflings-icon user"></i> Profile</a></li>
                                    <li><a href="@Url.Action("Login","Login")"><i class="halflings-icon off"></i>Logout</a></li>*@
                                <li>
                                    <a href="@Url.Action("Logout", "Home")"><i class="halflings-icon off"></i>Logout</a>
                                </li>
                            </ul>
                        </li>
                        <!-- end: User Dropdown -->
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <!-- start: Header -->
    <div class="container-fluid-full">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div id="sidebar-left" class="span2">
                <div class="nav-collapse sidebar-nav">
                    @*@Html.Partial("_MenuListing")*@
                </div>
            </div>
            <!-- end: Main Menu -->

            <div class="container body-content span10">

                <noscript>
                    <div class="alert alert-block span10" style="z-index:9999;">
                        <h4 class="alert-heading">Warning!</h4>
                        <p>
                            You need to have
                            <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a>
                            enabled to use this site.
                        </p>
                    </div>

                </noscript>



                @RenderBody()
                <hr />
            </div>
            <div class="clearfix"></div>
        </div><!--/#content.span10-->
    </div><!--/fluid-row-->
    <footer>
        <p>&copy; @DateTime.Now.Year - eZeelo</p>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <!-- start: JavaScript-->
    <!-- start: JavaScript-->
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    @*<script src="~/Content/js/jquery-2.1.3.min.js"></script>*@
    <script src="~/Content/js/jquery-migrate-1.0.0.min.js"></script>

    <script src="~/Content/js/jquery-ui-1.10.0.custom.min.js"></script>

    <script src="~/Content/js/jquery.ui.touch-punch.js"></script>

    <script src="~/Content/js/modernizr.js"></script>
    <script src="~/Content/js/bootstrap.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>

    <script src="~/Content/js/jquery.cookie.js"></script>

    <script src='~/Content/js/fullcalendar.min.js'></script>

    <script src='~/Content/js/jquery.dataTables.min.js'></script>
    @*<script src="~/Content/js/jquery-1.9.1.min.js"></script>*@
    <script src="~/Content/js/excanvas.js"></script>
    <script src="~/Content/js/jquery.flot.js"></script>
    <script src="~/Content/js/jquery.flot.pie.js"></script>
    <script src="~/Content/js/jquery.flot.stack.js"></script>
    <script src="~/Content/js/jquery.flot.resize.min.js"></script>

    <script src="~/Content/js/jquery.chosen.min.js"></script>

    <script src="~/Content/js/jquery.uniform.min.js"></script>

    <script src="~/Content/js/jquery.cleditor.min.js"></script>

    <script src="~/Content/js/jquery.noty.js"></script>

    <script src="~/Content/js/jquery.elfinder.min.js"></script>

    <script src="~/Content/js/jquery.raty.min.js"></script>


    <script src="~/Content/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/Content/js/jquery.gritter.min.js"></script>

    <script src="~/Content/js/jquery.imagesloaded.js"></script>

    <script src="~/Content/js/jquery.masonry.min.js"></script>

    <script src="~/Content/js/jquery.knob.modified.js"></script>

    <script src="~/Content/js/jquery.sparkline.min.js"></script>

    <script src="~/Content/js/counter.js"></script>

    <script src="~/Content/js/retina.js"></script>

    <script src="~/Content/js/custom.js"></script>
    <!-- end: JavaScript-->
    <script type="text/javascript">
        $(window).load(function () {

            if ($("#pageloaddiv").css('display') == 'none') {
                $("#pageloaddiv").css('display', 'block');
                $("#pageloaddiv").fadeOut(1000);
            }
        });
    </script>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".nav-stacked li a").click(function () {
                $(this).parent().parent().find('li.active').removeClass("active");
                $(this).parent().addClass("active");

            });

        });
    </script>
    @*<script type="text/javascript">
            $(window).load(function () {
                alert($(this).parent().parent().addClass('active1'));
               // var c = (".nav-stacked li ul li");
                if ($(".nav-stacked li ul li").hasClass('active')) {
                    $(".nav-stacked li ul li").parent().addClass('active1');
                }
            });
        </script>*@
</body>

</html>

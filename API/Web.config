  <!--Start Yashaswi 07-02-2019 For Log4net -->
  <log4net>
    <appender name="RollingFileAppenderError" type="log4net.Appender.RollingFileAppender" additivity="false">
      <file type="log4net.Util.PatternString" value="logs/Error/%date{yyyyMMdd}.log"/>
      <appendToFile value="true"/>
      <datePattern value="yyyyMMdd"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newlFine%exception"/>
      </layout>
      <param name="Threshold" value="ERROR"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR"/>
        <levelMax value="ERROR"/>
      </filter>
      <LevelRangeFilter minLevel="ERROR" maxLevel="ERROR" onMatch="ACCEPT" onMismatch="DENY"/>
    </appender>
    <appender name="RollingFileAppenderInfo" type="log4net.Appender.RollingFileAppender" additivity="false">
      <file type="log4net.Util.PatternString" value="logs/Info/%date{yyyyMMdd}.log"/>
      <appendToFile value="true"/>
      <datePattern value="yyyyMMdd"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newlFine%exception"/>
      </layout>
      <param name="Threshold" value="INFO"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="INFO"/>
        <levelMax value="INFO"/>
      </filter>
      <LevelRangeFilter minLevel="INFO" maxLevel="INFO" onMatch="ACCEPT" onMismatch="DENY"/>
    </appender>
    <appender name="RollingFileAppenderDebug" type="log4net.Appender.RollingFileAppender" additivity="false">
      <file type="log4net.Util.PatternString" value="logs/Debug/%date{yyyyMMdd}.log"/>
      <appendToFile value="true"/>
      <datePattern value="yyyyMMdd"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="10MB"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newlFine%exception"/>
      </layout>
      <param name="Threshold" value="DEBUG"/>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="DEBUG"/>
        <levelMax value="DEBUG"/>
      </filter>
      <LevelRangeFilter minLevel="DEBUG" maxLevel="DEBUG" onMatch="ACCEPT" onMismatch="DENY"/>
    </appender>
    <root>
      <appender-ref ref="RollingFileAppenderError" level="ERROR"/>
      <appender-ref ref="RollingFileAppenderInfo" level="INFO"/>
      <appender-ref ref="RollingFileAppenderDebug" level="DEBUG"/>
    </root>
  </log4net>
